<Window x:Class="Server_Designer.View.LoginForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Server_Designer.View"
        xmlns:login="clr-namespace:Base_MVVM.BaseLogin;assembly=Base_MVVM"
        mc:Ignorable="d"
        Title="LoginForm" Height="261.4" Width="568.2">

    <login:LoginUserControl LoginCommand="{Binding LoginCommand}" Login="{Binding Path=LoginStr,Mode=TwoWay}" CancelCommand="{Binding CancelCommand}" ></login:LoginUserControl>
   
</Window>
