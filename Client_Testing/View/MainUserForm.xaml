<Window x:Class="Client_Testing.View.MainUserForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client_Testing.View"
        mc:Ignorable="d"
        Title="MainUserForm" Height="450" Width="800">
    <Grid>
        <TabControl Margin="0" TabStripPlacement="Left">
            <TabItem Header="User">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="UserTests">
                <Grid Background="#FFE5E5E5">
                    <ListBox Height="177" Margin="10,10,306,0" VerticalAlignment="Top"/>
                </Grid>
            </TabItem>
            <TabItem Header="Statistics">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
